{
  "name": "covid-api",
  "version": "0.0.1",
  "description": "Scripts for manipulating covid-19 data",
  "main": "src/index",
  "scripts": {
    "dev": "NODE_ENV=development printf '\\33c\\e[3J' & yarn type-watch & yarn watch",
    "watch": "babel-watch src --out-dir dist --extensions '.ts,.js' --watch",
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "type-watch": "tsc-watch  -p tsconfig.json --noEmit",
    "type-check": "tsc  -p tsconfig.json --noEmit",
    "build": "yarn clean && tsc -p tsconfig.json",
    "clean": "rm -rf ./dist",
    "lint": "yarn type-check && eslint src --ext .js,.ts",
    "lint:fix": "yarn type-check && eslint src --ext .js,.ts --quiet --fix "
  },
  "author": "Barry Low <barry@studiobear.co>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "cron": "^1.8.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "ftps": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/ftps": "^1.1.0",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.19.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 2,
          "targets": {
            "node": true
          }
        }
      ],
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  }
}
